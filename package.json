    "grpc": "^1.19.0",
    "jsonwebtoken": "^8.5.0",
    "neo4j-driver": "^1.7.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "nodemon": "~1.18.9"
