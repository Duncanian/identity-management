{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [false],
    "no-console": [false],
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "semicolon": false,
    "no-var-requires": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": [false],
    "jsdoc-format": [false]
  },
  "rulesDirectory": []
}
